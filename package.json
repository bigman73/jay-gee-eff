{
  "name": "jay-gee-eff",
  "version": "2.6.0",
  "description": "JGF - JSON Graph Format manipulation module for JavaScript. Reads and writes JGF files.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "jest --config jestconfig.json",
    "lint": "npx eslint **/*.ts && npm run prettier:check",
    "lint:fix": "eslint --fix **/*.ts && npm run prettier:fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "upg": "npx npm-check-updates --interactive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigman73/jay-gee-eff.git"
  },
  "keywords": [
    "JGF",
    "jsongraph",
    "graph",
    "nodes",
    "vertices",
    "edges",
    "relationships",
    "javascript",
    "es7"
  ],
  "author": "Bigman73 (https://github.com/bigman73/jay-gee-eff)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigman73/jay-gee-eff/issues"
  },
  "homepage": "https://github.com/bigman73/jay-gee-eff#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/check-types": "^7.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/lodash.isempty": "^4.4.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "npm-check-updates": "^16.3.22",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "check-types": "^11.1.2",
    "fast-clone": "^1.5.13",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "jsonschema": "^1.4.1",
    "lodash.isempty": "^4.4.0"
  }
}
